<!DOCTYPE html>
<html lang="es">

<%- include('./partials/head',{title: 'Mundo Pets' }) %>

    <body class="productos">
        <%- include('./partials/header') %>
            <main>
                <h1>NUESTROS PRODUCTOS</h1>
                <section class="allProductos">
                    <%productos.forEach(producto=> { %>
                        <%- include('./partials/cards',{ id:producto.id, imagenes:producto.imagen,
                            nombre:producto.titulo, descuento:producto.descuento, precio:producto.precio }) %>
                            <% }) %>

                </section>

            </main>
            <%- include('./partials/footer') %>
    </body>

</html>